<Window x:Class="PR__13.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PR__13"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="900">
    <Grid>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="120*"/>
                <RowDefinition Height="350*"/>
                <RowDefinition Height="30*"/>
            </Grid.RowDefinitions>
            <Canvas Background="Aqua">
                <Image Source="/Resources/512.ico" Height="96" Width="111"/>
            </Canvas>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <TextBlock Text="Склад" Style="{StaticResource Title}"/>
            </StackPanel>
            <Frame x:Name="FrmMain" NavigationUIVisibility="Hidden" Grid.Row="1"/>
            
            <Grid Background="Aqua" Grid.Row="2"></Grid>
        </Grid>
        
    </Grid>
</Window>
